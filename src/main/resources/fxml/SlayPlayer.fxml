<StackPane fx:id="stackPane" prefHeight="1792.0" prefWidth="828.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fluffyluffs.slayplayer.controller.SlayPlayerController">
